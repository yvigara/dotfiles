{
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.suggestSelection": "first",
  "editor.snippetSuggestions": "top",
  "editor.fontSize": 13,
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line",
  "editor.scrollBeyondLastLine": false,
  "editor.formatOnPaste": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
    // "source.sortImports": true
  },
  // Workbench
  "workbench.colorTheme": "Gruvbox Dark Medium",
  "workbench.preferredDarkColorTheme": "Gruvbox Dark Medium",
  "workbench.preferredLightColorTheme": "Gruvbox Light Medium",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  // Window
  "window.newWindowDimensions": "maximized",
  "window.title": "${activeEditorMedium} ${dirty}",
  "window.zoomLevel": 0,
  // Material Icon
  "material-icon-theme.showWelcomeMessage": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "material-icon-theme.activeIconPack": "react_redux",
  "beautify.language": {
    "js": []
  },
  // Files
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/bower_components": true,
    "**/.sass-cache": true,
    ".nyc_output": true
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    // Ansible
    "**/defaults/**/*.yml": "jinja-yaml",
    "**/group_vars/**/*": "jinja-yaml",
    "**/host_vars/**/*": "jinja-yaml",
    "**/vars/**/*.yml": "jinja-yaml",
    "**/tasks/**/*.yml": "ansible",
    "**/handlers/*.yml": "ansible",
    "**/meta/*.yml": "ansible",
    "**/roles/**/*.yml": "ansible",
    "**/playbooks/**/*.yml": "ansible",
    "**/ansible/**/hosts": "ini",
    "**/ansible/**/inventory": "ini",
    "ansible.cfg": "ini"
  },
  // Golang
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--disable-all",
    "--enable=vet",
    "--enable=vetshadow",
    "--enable=golint",
    "--enable=ineffassign",
    "--enable=goconst",
    "--enable=gofmt",
    "--enable=goimports",
    "--deadline=2m",
    "--enable=bodyclose",
    "--enable=gosimple",
    "--enable=whitespace"
  ],
  "go.testFlags": ["-v"],
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "go.languageServerFlags": [],
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.autocompleteUnimportedPackages": true,
  "gopls": {
    "usePlaceholders": true, // add parameter placeholders when completing a function
    // Experimental settings
    "completeUnimported": true, // autocomplete unimported packages
    "ui.semanticTokens": true,
    "deepCompletion": true // enable deep completion
  },
  "[go]": {
    "editor.snippetSuggestions": "none",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true,
      "source.sortImports": true
    }
  },
  // Integrated terminal
  "terminal.external.osxExec": "iterm2",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "600",
  // Breadcrumbs
  "breadcrumbs.enabled": false,
  // SCM
  "scm.defaultViewMode": "tree",
  // Vim
  "vim.leader": "<space>",
  "vim.incsearch": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.surround": false,
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.enableNeovim": true,
  // Explorer
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.badges": false,
  "explorer.openEditors.visible": 0,
  "explorer.confirmDelete": false,
  "yaml.format.enable": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.surveys.enabled": false,
  "extensions.ignoreRecommendations": true,
  // Powershell
  "powershell.codeFormatting.openBraceOnSameLine": false,
  "powershell.codeFormatting.newLineAfterOpenBrace": true,
  "powershell.codeFormatting.newLineAfterCloseBrace": true,
  "powershell.codeFormatting.whitespaceBeforeOpenBrace": true,
  "powershell.codeFormatting.whitespaceBeforeOpenParen": true,
  "powershell.codeFormatting.whitespaceAroundOperator": true,
  "powershell.codeFormatting.whitespaceAfterSeparator": true,
  "powershell.codeFormatting.ignoreOneLineBlock": false,
  "powershell.codeFormatting.useConstantStrings": true,
  "powershell.codeFormatting.useCorrectCasing": true,
  "powershell.codeFormatting.pipelineIndentationStyle": "IncreaseIndentationAfterEveryPipeline",
  "powershell.codeFormatting.alignPropertyValuePairs": true,
  "powershell.scriptAnalysis.enable": true,
  // Python
  "python.jediEnabled": false,
  "python.languageServer": "Pylance",
  "python.linting.pylintEnabled": true,
  "python.formatting.provider": "yapf",
  "material-ui-snippets.showNotesOnStartup": false,
  "python.showStartPage": false,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "editor.semanticHighlighting.enabled": true,
  "editor.semanticTokenColorCustomizations": {
    "[Gruvbox Dark Medium]": {
      "enabled": true,
      "rules": {
        "parameter": {
          "foreground": "#83a598",
          "fontStyle": "italic"
        },
        "type": {
          "fontStyle": "underline"
        },
        "interface": {
          "foreground": "#d79921"
        },
        "storage.type": {
          "foreground": "#0FF000",
          "fontStyle": "italic"
        },
        "variable.other.readwrite.alias": {
          "foreground": "#a89984"
        },
        "meta.import": {
          "foreground": "#a89984"
        },
        "namespace": {
          "foreground": "#8EC07C"
        },
        "string": {
          "foreground": "#90A4AE"
        },
        "function": {
          "foreground": "#babb26"
          // "fontStyle": "bold"
        },
        "keyword": "#b16286",
        "struct": "#d79921",
        "class": "#f07178",
        "class.readonly": {
          "foreground": "#ffd580",
          "fontStyle": ""
        },
        "class.defaultLibrary": {
          "foreground": "#d79921",
          "fontStyle": "italic"
        },
        "class.declaration": {
          "foreground": "#83a598",
          "fontStyle": ""
        },
        "enum": "#9dd2bb",
        "macro": "#ffd580",
        "variable": "#83A598",
        "variable.declaration": {
          "foreground": "#83A598",
          "fontStyle": ""
        },
        "variable.declaration.readonly": {
          "foreground": "#d3869b",
          "fontStyle": "italic"
        },
        "variable.readonly": {
          "foreground": "#d3869b",
          "fontStyle": "italic"
        },
        "variable.defaultLibrary": "#458588",
        "typeParameter": "#83a598",
        "property": "#ebdbb2",
        "property.declaration": "#ebdbb2",
        "property.readonly": "#d3869b",
        "property.defaultLibrary": {
          "foreground": "#ebdbb2",
          "fontStyle": ""
        },
        "enumMember": "#9dd2bb",
        "event": "#83a598",
        "label": "#b3b1ad",
        "comment": "#928374",
        "number": "#9dd2bb",
        "regexp": "#95e6cb",
        "operator": "#fb4934",
        "0": "#282828",
        "1": "#cc241d",
        "2": "#98971a",
        "3": "#d79921",
        "4": "#458588",
        "5": "#b16286",
        "6": "#689d6a",
        "7": "#a89984",
        "8": "#928374",
        "9": "#fb4934",
        "10": "#b8bb26",
        "11": "#fabd2f",
        "12": "#83a598",
        "13": "#d3869b",
        "15": "#8ec07c",
        "x": "#ebdbb2"
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "[Gruvbox Dark Medium]": {
      "textMateRules": [
        {
          "scope": ["string"],
          "settings": {
            "foreground": "#98971a"
          }
        },
        {
          "scope": ["storage.type"],
          "settings": {
            "foreground": "#fb4934",
            "fontStyle": "italic"
          }
        }
      ]
    }
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "redhat.telemetry.enabled": false,
  "editor.tabSize": 8,
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "docker.showStartPage": false,
  "editor.bracketPairColorization.enabled": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.guides.bracketPairs": true,
  "telemetry.telemetryLevel": "off",
  "workbench.startupEditor": "none",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript", // if you are using typescript
    "typescriptreact": "javascript" // if you are using typescript with react
  },
  "editor.quickSuggestions": {
    "strings": true // forces VS Code to trigger completions when editing "string" content
  },
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    "tw\\.[^`]+`([^`]*)`",
    "tw\\(.*?\\).*?`([^`]*)"
  ],
  "editor.stickyScroll.enabled": true
  // "eslint.format.enable": true
}
